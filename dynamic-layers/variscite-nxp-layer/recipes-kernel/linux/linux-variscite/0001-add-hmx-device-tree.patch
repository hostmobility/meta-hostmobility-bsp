+/* First Automotive Ethernet Marvell 88Q2110 */
+&fec {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_fec>;
+	phy-mode = "rgmii-id";
+	phy-handle = <&ethphy_fec>;
+	status = "okay";
+
+	mdio {
+		#address-cells = <1>;
+		#size-cells = <0>;
+
+		/* Shared reset GPIO between both PHYs on GPIO3_1
+
+		The reset line is shared between both 88Q2110 PHYs, but Linux's
+		device model doesn't support claiming the same GPIO from multiple
+		drivers. Therefore, reset configuration is omitted from PHY nodes.
+
+		With proper pinctrl, PHYs perform hardware strapping during power-up
+		by reading RXD[0:1] and GPIO pins to auto-configure MDIO registers, 
+		making explicit reset unnecessary.
+
+		Also possible to configure speed + master/slave with RXD[2:3] and 
+		RGMII mode with RXC. This can also be configure by software. 
+
+		For reference, shared reset would be:
+		  reset-gpios = <&gpio3 1 GPIO_ACTIVE_HIGH>;
+		  reset-delay-us = <100000>;       // 100ms reset assertion
+		  reset-post-delay-us = <200000>;  // 200ms post-reset delay */
+
+		ethphy_fec: ethernet-phy@3 {
+			compatible = "ethernet-phy-id002b.0983";
+			reg = <3>;
+			max-speed = <1000>;
+		};
+
+		ethphy_eqos: ethernet-phy@7 {
+			compatible = "ethernet-phy-id002b.0983";
+			reg = <7>;
+			max-speed = <1000>;
+		};
+	};
+};
+
+/* Second Automotive Ethernet Marvell 88Q2110 */
