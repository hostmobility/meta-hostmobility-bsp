+	pinctrl-0 = <&pinctrl_eqos>;
+	phy-mode = "rgmii";
+	phy-handle = <&ethphy_eqos>;
+	status = "okay";
+
+	/* EQOS shares the MDIO bus configured under FEC node above.
+	Remove the redundant MDIO node to avoid conflicts. */
+	/delete-node/ mdio;
+
+	/* Performance tuning options (currently disabled):
+	If EQOS have performance issues, consider enabling DMA threshold
+	mode or custom MTL TX/RX configurations. See snps,dwmac.yaml
+	binding documentation for available options.
+
+	Example: snps,force_thresh_dma_mode; */
+};
+
+&i2c2 {
+	clock-frequency = <100000>;
+	pinctrl-names = "default", "gpio";
+	pinctrl-0 = <&pinctrl_i2c2>;
+	pinctrl-1 = <&pinctrl_i2c2_gpio>;
+	status = "okay";
