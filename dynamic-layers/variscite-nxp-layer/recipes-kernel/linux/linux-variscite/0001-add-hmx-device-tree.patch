+	pinctrl_eqos: eqosgrp {
+		fsl,pins = <
+			MX8MP_IOMUXC_ENET_MDC__ENET_QOS_MDC				0x2
+			MX8MP_IOMUXC_ENET_MDIO__ENET_QOS_MDIO				0x2
+			MX8MP_IOMUXC_ENET_RD0__ENET_QOS_RGMII_RD0			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_ENET_RD1__ENET_QOS_RGMII_RD1			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_ENET_RD2__ENET_QOS_RGMII_RD2			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_ENET_RD3__ENET_QOS_RGMII_RD3			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
