+			MX8MP_IOMUXC_SAI1_RXD2__ENET1_MDC				0x2
+			MX8MP_IOMUXC_SAI1_RXD3__ENET1_MDIO				0x2
+			MX8MP_IOMUXC_SAI1_RXD4__ENET1_RGMII_RD0			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_SAI1_RXD5__ENET1_RGMII_RD1			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_SAI1_RXD6__ENET1_RGMII_RD2			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_SAI1_RXD7__ENET1_RGMII_RD3			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_SAI1_TXC__ENET1_RGMII_RXC			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_0_WEAK_PULL_DOWN)
+			MX8MP_IOMUXC_SAI1_TXFS__ENET1_RGMII_RX_CTL			(FSEL_1_FAST_SLEW_RATE | HYS_1_SCHMITT | PE_1_PULL_ENABLE | PUE_1_WEAK_PULL_UP)
+			MX8MP_IOMUXC_SAI1_TXD0__ENET1_RGMII_TD0			0x16
+			MX8MP_IOMUXC_SAI1_TXD1__ENET1_RGMII_TD1			0x16
+			MX8MP_IOMUXC_SAI1_TXD2__ENET1_RGMII_TD2			0x16
+			MX8MP_IOMUXC_SAI1_TXD3__ENET1_RGMII_TD3			0x16
+			MX8MP_IOMUXC_SAI1_TXD4__ENET1_RGMII_TX_CTL			0x16
+			MX8MP_IOMUXC_SAI1_TXD5__ENET1_RGMII_TXC			0x16
+			MX8MP_IOMUXC_NAND_CE0_B__GPIO3_IO01		(PE_1_PULL_ENABLE  | PUE_0_WEAK_PULL_DOWN | DSE_X2)
+		>;
+	};
